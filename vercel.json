{
  "version": 2,
  "installCommand": "",
  "buildCommand": "",
  "outputDirectory": ".vercel/output",
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)\\.(js|css|svg|png|jpg|jpeg|gif|webp|ico|woff2?)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.(html|json)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "functions": {
    "api/**": {
      "maxDuration": 10
    }
  }
}
